extends layout

block content

    .w3-card-4
        header.w3-container.w3-pale-red
            h1 Planta ##{plant._id}

        .w3-container
            ul.w3-ul.w3-card-4
                li #[b Número de Registo:] #{plant["Número de Registo"]}
                li #[b Código de rua:] #{plant["Código de rua"]}
                li #[b Rua:] #{plant["Rua"]}
                li #[b Local:] #{plant["Local"]}
                li #[b Freguesia:] #{plant["Freguesia"]}
                li #[b Espécie:] #{plant["Espécie"]}
                li #[b Nome Científico:] #{plant["Nome Científico"]}
                li #[b Origem:] #{plant["Origem"]}
                li #[b Data de Plantação:] #{plant["Data de Plantação"]}
                li #[b Estado:] #{plant["Estado"]}
                li #[b Caldeira:] #{plant["Caldeira"]}
                li #[b Tutor:] #{plant["Tutor"]}
                li #[b Implantação:] #{plant["Implantação"]}
                li #[b Gestor:] #{plant["Gestor"]}
                li #[b Data de actualização:] #{plant["Data de actualização"]}
                li #[b Número de intervenções:] #{plant["Número de intervenções"]}

    footer.w3-container.w3-pale-red
            h5 Generated by Guilherme Sampaio - #[a(href="/") Voltar à página principal]



